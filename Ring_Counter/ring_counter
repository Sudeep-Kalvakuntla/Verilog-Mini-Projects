:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x636f4aba5550 .scope module, "ring_counter_tb" "ring_counter_tb" 2 3;
 .timescale -9 -12;
P_0x636f4aba56e0 .param/l "N" 0 2 4, +C4<00000000000000000000000000000100>;
v0x636f4abf2180_0 .var "clk", 0 0;
v0x636f4abf2250_0 .net "q", 3 0, v0x636f4abf1f50_0;  1 drivers
v0x636f4abf2320_0 .var "rst", 0 0;
E_0x636f4abe01d0 .event posedge, v0x636f4abdff70_0;
S_0x636f4abdfc50 .scope module, "uut" "ring_counter" 2 11, 3 2 0, S_0x636f4aba5550;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "q";
P_0x636f4abdfe30 .param/l "N" 0 3 3, +C4<00000000000000000000000000000100>;
v0x636f4abdff70_0 .net "clk", 0 0, v0x636f4abf2180_0;  1 drivers
v0x636f4abf1f50_0 .var "q", 3 0;
v0x636f4abf2030_0 .net "rst", 0 0, v0x636f4abf2320_0;  1 drivers
E_0x636f4abe03f0 .event posedge, v0x636f4abf2030_0, v0x636f4abdff70_0;
    .scope S_0x636f4abdfc50;
T_0 ;
    %wait E_0x636f4abe03f0;
    %load/vec4 v0x636f4abf2030_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x636f4abf1f50_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x636f4abf1f50_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x636f4abf1f50_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x636f4abf1f50_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x636f4aba5550;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x636f4abf2180_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x636f4abf2180_0;
    %inv;
    %store/vec4 v0x636f4abf2180_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x636f4aba5550;
T_2 ;
    %vpi_call 2 23 "$dumpfile", "ring_counter.vcd" {0 0 0};
    %vpi_call 2 24 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x636f4aba5550 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x636f4abf2320_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x636f4abf2320_0, 0, 1;
    %pushi/vec4 8, 0, 32;
T_2.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_2.1, 5;
    %jmp/1 T_2.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %wait E_0x636f4abe01d0;
    %jmp T_2.0;
T_2.1 ;
    %pop/vec4 1;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
