S_0x5cd41c2e1520 .scope module, "asynchronous_counter_tb" "asynchronous_counter_tb" 2 3;
 .timescale -9 -12;
v0x5cd41c330a50_0 .var "clk", 0 0;
v0x5cd41c330af0_0 .net "q", 3 0, v0x5cd41c330850_0;  1 drivers
v0x5cd41c330bc0_0 .var "rst", 0 0;
S_0x5cd41c2e16b0 .scope module, "uut" "asynchronous_counter" 2 7, 3 1 0, S_0x5cd41c2e1520;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "q";
v0x5cd41c31bcf0_0 .net "clk", 0 0, v0x5cd41c330a50_0;  1 drivers
v0x5cd41c330850_0 .var "q", 3 0;
v0x5cd41c330930_0 .net "rst", 0 0, v0x5cd41c330bc0_0;  1 drivers
E_0x5cd41c31c5f0 .event posedge, v0x5cd41c330930_0, v0x5cd41c31bcf0_0;
    .scope S_0x5cd41c2e16b0;
T_0 ;
    %wait E_0x5cd41c31c5f0;
    %load/vec4 v0x5cd41c330930_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5cd41c330850_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 0, 2;
    %inv;
    %ix/load 4, 0, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5cd41c330850_0, 4, 5;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 0, 2;
    %flag_set/vec4 8;
    %jmp/0 T_0.2, 8;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 1, 2;
    %inv;
    %jmp/1 T_0.3, 8;
T_0.2 ; End of true expr.
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 1, 2;
    %jmp/0 T_0.3, 8;
 ; End of false expr.
    %blend;
T_0.3;
    %ix/load 4, 1, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5cd41c330850_0, 4, 5;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 0, 2;
    %and;
    %flag_set/vec4 8;
    %jmp/0 T_0.4, 8;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 2, 3;
    %inv;
    %jmp/1 T_0.5, 8;
T_0.4 ; End of true expr.
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 2, 3;
    %jmp/0 T_0.5, 8;
 ; End of false expr.
    %blend;
T_0.5;
    %ix/load 4, 2, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5cd41c330850_0, 4, 5;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 1, 2;
    %and;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 0, 2;
    %and;
    %flag_set/vec4 8;
    %jmp/0 T_0.6, 8;
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 3, 3;
    %inv;
    %jmp/1 T_0.7, 8;
T_0.6 ; End of true expr.
    %load/vec4 v0x5cd41c330850_0;
    %parti/s 1, 3, 3;
    %jmp/0 T_0.7, 8;
 ; End of false expr.
    %blend;
T_0.7;
    %ix/load 4, 3, 0;
    %ix/load 5, 0, 0;
    %flag_set/imm 4, 0;
    %assign/vec4/off/d v0x5cd41c330850_0, 4, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5cd41c2e1520;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cd41c330a50_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5cd41c330a50_0;
    %inv;
    %store/vec4 v0x5cd41c330a50_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5cd41c2e1520;
T_2 ;
    %vpi_call 2 19 "$dumpfile", "asynchronous_counter.vcd" {0 0 0};
    %vpi_call 2 20 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5cd41c2e1520 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cd41c330bc0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5cd41c330bc0_0, 0, 1;
    %delay 65000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5cd41c330bc0_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
