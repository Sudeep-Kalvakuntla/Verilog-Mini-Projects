:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x6548dc7ef520 .scope module, "_0247_sequence_tb" "_0247_sequence_tb" 2 3;
 .timescale -9 -12;
v0x6548dc83c1b0_0 .var "clk", 0 0;
v0x6548dc83c280_0 .net "q", 2 0, v0x6548dc83bfb0_0;  1 drivers
v0x6548dc83c350_0 .var "rst", 0 0;
S_0x6548dc7ef6b0 .scope module, "uut" "_0247_sequence" 2 8, 3 3 0, S_0x6548dc7ef520;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 3 "q";
v0x6548dc829df0_0 .net "clk", 0 0, v0x6548dc83c1b0_0;  1 drivers
v0x6548dc83bfb0_0 .var "q", 2 0;
v0x6548dc83c090_0 .net "rst", 0 0, v0x6548dc83c350_0;  1 drivers
E_0x6548dc82a6c0 .event posedge, v0x6548dc83c090_0, v0x6548dc829df0_0;
    .scope S_0x6548dc7ef6b0;
T_0 ;
    %wait E_0x6548dc82a6c0;
    %load/vec4 v0x6548dc83c090_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x6548dc83bfb0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6548dc83bfb0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 3;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 3;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x6548dc83bfb0_0, 0;
    %jmp T_0.7;
T_0.2 ;
    %pushi/vec4 2, 0, 3;
    %assign/vec4 v0x6548dc83bfb0_0, 0;
    %jmp T_0.7;
T_0.3 ;
    %pushi/vec4 4, 0, 3;
    %assign/vec4 v0x6548dc83bfb0_0, 0;
    %jmp T_0.7;
T_0.4 ;
    %pushi/vec4 7, 0, 3;
    %assign/vec4 v0x6548dc83bfb0_0, 0;
    %jmp T_0.7;
T_0.5 ;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x6548dc83bfb0_0, 0;
    %jmp T_0.7;
T_0.7 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6548dc7ef520;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6548dc83c1b0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x6548dc83c1b0_0;
    %inv;
    %store/vec4 v0x6548dc83c1b0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x6548dc7ef520;
T_2 ;
    %vpi_call 2 20 "$dumpfile", "0247_sequence.vcd" {0 0 0};
    %vpi_call 2 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6548dc7ef520 {0 0 0};
    %delay 6000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6548dc83c350_0, 0, 1;
    %delay 4000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6548dc83c350_0, 0, 1;
    %delay 55000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6548dc83c350_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6548dc83c350_0, 0, 1;
    %delay 15000, 0;
    %vpi_call 2 34 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
