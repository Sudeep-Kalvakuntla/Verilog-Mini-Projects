:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x5ddfc3986520 .scope module, "gray_code_counter_tb" "gray_code_counter_tb" 2 3;
 .timescale -9 -12;
v0x5ddfc39d4980_0 .var "clk", 0 0;
v0x5ddfc39d4a50_0 .net "q", 2 0, v0x5ddfc39d4780_0;  1 drivers
v0x5ddfc39d4b20_0 .var "rst", 0 0;
S_0x5ddfc39866b0 .scope module, "uut" "gray_code_counter" 2 9, 3 3 0, S_0x5ddfc3986520;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 3 "q";
v0x5ddfc39c0d90_0 .var "bin", 2 0;
v0x5ddfc39d46c0_0 .net "clk", 0 0, v0x5ddfc39d4980_0;  1 drivers
v0x5ddfc39d4780_0 .var "q", 2 0;
v0x5ddfc39d4840_0 .net "rst", 0 0, v0x5ddfc39d4b20_0;  1 drivers
E_0x5ddfc39c1e80 .event anyedge, v0x5ddfc39c0d90_0;
E_0x5ddfc39c1410 .event posedge, v0x5ddfc39d4840_0, v0x5ddfc39d46c0_0;
    .scope S_0x5ddfc39866b0;
T_0 ;
    %wait E_0x5ddfc39c1410;
    %load/vec4 v0x5ddfc39d4840_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x5ddfc39c0d90_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5ddfc39c0d90_0;
    %addi 1, 0, 3;
    %store/vec4 v0x5ddfc39c0d90_0, 0, 3;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5ddfc39866b0;
T_1 ;
    %wait E_0x5ddfc39c1e80;
    %load/vec4 v0x5ddfc39c0d90_0;
    %parti/s 1, 2, 3;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ddfc39d4780_0, 4, 1;
    %load/vec4 v0x5ddfc39c0d90_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x5ddfc39c0d90_0;
    %parti/s 1, 1, 2;
    %xor;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ddfc39d4780_0, 4, 1;
    %load/vec4 v0x5ddfc39c0d90_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x5ddfc39c0d90_0;
    %parti/s 1, 0, 2;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x5ddfc39d4780_0, 4, 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x5ddfc3986520;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ddfc39d4980_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5ddfc39d4980_0;
    %inv;
    %store/vec4 v0x5ddfc39d4980_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5ddfc3986520;
T_3 ;
    %vpi_call 2 21 "$dumpfile", "gray_code_counter.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5ddfc3986520 {0 0 0};
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ddfc39d4b20_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ddfc39d4b20_0, 0, 1;
    %delay 125000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5ddfc39d4b20_0, 0, 1;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5ddfc39d4b20_0, 0, 1;
    %delay 40000, 0;
    %vpi_call 2 34 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
