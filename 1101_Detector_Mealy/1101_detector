E_0x58c094aeb620 .event posedge, v0x58c094b02c10_0, v0x58c094aeab20_0;
E_0x58c094aebe30 .event anyedge, v0x58c094b02870_0, v0x58c094b02950_0;
    .scope S_0x58c094aaf6b0;
T_0 ;
    %wait E_0x58c094aebe30;
