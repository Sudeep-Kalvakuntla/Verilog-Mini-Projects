    %store/vec4 v0x58c094b02a20_0, 0, 2;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x58c094b02b00_0, 0, 1;
    %load/vec4 v0x58c094b02870_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
