    %pushi/vec4 3, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x58c094b02a20_0, 0, 2;
    %jmp T_0.5;
T_0.0 ;
    %load/vec4 v0x58c094b02950_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x58c094b02a20_0, 0, 2;
T_0.6 ;
    %jmp T_0.5;
T_0.1 ;
    %load/vec4 v0x58c094b02950_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.8, 8;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x58c094b02a20_0, 0, 2;
    %jmp T_0.9;
T_0.8 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x58c094b02a20_0, 0, 2;
T_0.9 ;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x58c094b02950_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.10, 8;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x58c094b02a20_0, 0, 2;
T_0.10 ;
    %jmp T_0.5;
T_0.3 ;
    %load/vec4 v0x58c094b02950_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.12, 8;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x58c094b02a20_0, 0, 2;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x58c094b02b00_0, 0, 1;
    %jmp T_0.13;
T_0.12 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x58c094b02a20_0, 0, 2;
T_0.13 ;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x58c094aaf6b0;
T_1 ;
    %wait E_0x58c094aeb620;
    %load/vec4 v0x58c094b02c10_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x58c094b02870_0, 0;
