v0x5e8372ecc5a0_0 .net "rdData", 15 0, v0x5e8372ecc2a0_0;  1 drivers
S_0x5e8372eb6970 .scope begin, "$ivl_for_loop0" "$ivl_for_loop0" 2 31, 2 31 0, S_0x5e8372ea3360;
 .timescale -9 -12;
v0x5e8372ea5220_0 .var/i "i", 31 0;
S_0x5e8372ecbc80 .scope module, "uut" "rom" 2 17, 3 1 0, S_0x5e8372ea3360;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 10 "rdAddr";
    .port_info 2 /OUTPUT 16 "rdData";
P_0x5e8372ea1f10 .param/l "Depth" 0 3 3, +C4<00000000000000000000010000000000>;
P_0x5e8372ea1f50 .param/l "Width" 0 3 2, +C4<00000000000000000000000000010000>;
v0x5e8372ecc010_0 .net "clk", 0 0, v0x5e8372ecc400_0;  1 drivers
v0x5e8372ecc0f0 .array "mem", 0 1023, 15 0;
v0x5e8372ecc1b0_0 .net "rdAddr", 9 0, v0x5e8372ecc4d0_0;  1 drivers
v0x5e8372ecc2a0_0 .var "rdData", 15 0;
E_0x5e8372eb5de0 .event posedge, v0x5e8372ecc010_0;
    .scope S_0x5e8372ecbc80;
T_0 ;
    %vpi_call 3 13 "$readmemb", "initData.mif", v0x5e8372ecc0f0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5e8372ecbc80;
T_1 ;
    %wait E_0x5e8372eb5de0;
    %load/vec4 v0x5e8372ecc1b0_0;
    %pad/u 12;
    %ix/vec4 4;
    %load/vec4a v0x5e8372ecc0f0, 4;
    %assign/vec4 v0x5e8372ecc2a0_0, 0;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5e8372ea3360;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5e8372ecc400_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5e8372ecc400_0;
    %inv;
    %store/vec4 v0x5e8372ecc400_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x5e8372ea3360;
T_3 ;
    %vpi_call 2 24 "$dumpfile", "rom.vcd" {0 0 0};
    %vpi_call 2 25 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5e8372ea3360 {0 0 0};
    %pushi/vec4 0, 0, 10;
    %store/vec4 v0x5e8372ecc4d0_0, 0, 10;
    %delay 20000, 0;
    %fork t_1, S_0x5e8372eb6970;
    %jmp t_0;
    .scope S_0x5e8372eb6970;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5e8372ea5220_0, 0, 32;
T_3.0 ;
    %load/vec4 v0x5e8372ea5220_0;
    %cmpi/s 10, 0, 32;
    %jmp/0xz T_3.1, 5;
    %load/vec4 v0x5e8372ea5220_0;
    %pad/s 10;
    %store/vec4 v0x5e8372ecc4d0_0, 0, 10;
    %delay 10000, 0;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x5e8372ea5220_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x5e8372ea5220_0, 0, 32;
    %jmp T_3.0;
T_3.1 ;
    %end;
    .scope S_0x5e8372ea3360;
t_0 %join;
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x5e8372ea3360;
T_4 ;
    %wait E_0x5e8372eb5de0;
    %vpi_call 2 42 "$display", "Time: %0t | Addr: %0d | Data: %0h", $time, v0x5e8372ecc4d0_0, v0x5e8372ecc5a0_0 {0 0 0};
    %jmp T_4;
    .thread T_4;
# The file index is used to find the file name in the following table.
