:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x5b09e2acd540 .scope module, "sipo_shift_tb" "sipo_shift_tb" 2 3;
 .timescale -9 -12;
v0x5b09e2b1abb0_0 .var "clk", 0 0;
v0x5b09e2b1ac70_0 .net "q", 3 0, v0x5b09e2b1a890_0;  1 drivers
v0x5b09e2b1ad40_0 .var "rst", 0 0;
v0x5b09e2b1ae40_0 .var "serial_in", 0 0;
S_0x5b09e2acd6d0 .scope module, "uut" "sipo_shift" 2 8, 3 1 0, S_0x5b09e2acd540;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "serial_in";
    .port_info 3 /OUTPUT 4 "q";
v0x5b09e2b08b40_0 .net "clk", 0 0, v0x5b09e2b1abb0_0;  1 drivers
v0x5b09e2b1a890_0 .var "q", 3 0;
v0x5b09e2b1a970_0 .net "rst", 0 0, v0x5b09e2b1ad40_0;  1 drivers
v0x5b09e2b1aa40_0 .net "serial_in", 0 0, v0x5b09e2b1ae40_0;  1 drivers
E_0x5b09e2b091c0 .event posedge, v0x5b09e2b1a970_0, v0x5b09e2b08b40_0;
    .scope S_0x5b09e2acd6d0;
T_0 ;
    %wait E_0x5b09e2b091c0;
    %load/vec4 v0x5b09e2b1a970_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5b09e2b1a890_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5b09e2b1aa40_0;
    %load/vec4 v0x5b09e2b1a890_0;
    %parti/s 3, 1, 2;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5b09e2b1a890_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5b09e2acd540;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b09e2b1abb0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5b09e2b1abb0_0;
    %inv;
    %store/vec4 v0x5b09e2b1abb0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5b09e2acd540;
T_2 ;
    %vpi_call 2 21 "$dumpfile", "sipo_shift.vcd" {0 0 0};
    %vpi_call 2 22 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5b09e2acd540 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b09e2b1ae40_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b09e2b1ad40_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b09e2b1ad40_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b09e2b1ae40_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b09e2b1ae40_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b09e2b1ae40_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b09e2b1ae40_0, 0, 1;
    %delay 30000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5b09e2b1ad40_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5b09e2b1ad40_0, 0, 1;
    %delay 5000, 0;
    %vpi_call 2 42 "$finish" {0 0 0};
    %end;
    .thread T_2;
